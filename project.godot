; Format: https://docs.godotengine.org/en/stable/classes/class_projectsettings.html#class-projectsettings

config_version=5

[application]
config/name="Idle God RPG"
run/main_scene="res://scenes/Main.tscn"
config/features=PackedStringArray("4.0", "Mobile")
config/icon=""

[display]
window/size/viewport_width=1280
window/size/viewport_height=720
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"
window/handheld/orientation="landscape"

[input]
ui_time_pause={"deadzone":0.5,"events":[{"device":0,"button_index":16777217,"deadzone":0.5,"type":"InputEventKey"}]}
ui_time_play={"deadzone":0.5,"events":[{"device":0,"button_index":16777218,"deadzone":0.5,"type":"InputEventKey"}]}

[autoload]
ItemDB="*res://scripts/item_db.gd"
TraitDB="*res://scripts/trait_db.gd"
LocationDB="*res://scripts/location_db.gd"
BlessingDB="*res://scripts/blessing_db.gd"
GameState="*res://scripts/game_state.gd"

[rendering]
renderer/rendering_method="mobile_forward_plus"
renderer/rendering_method.mobile="mobile_forward_plus"

